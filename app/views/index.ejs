    <body>
        <!--[if lte IE 9]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
        <![endif]-->

        <div class='main-content'>
          <h3>Hey, here's the list:</h3>
          <%if(messages.length>0){%>
            <ul>
              <% messages.forEach((message)=>{ %>
              <%-`<a href=${message.link}><li>${message.name}</li></a>`%>
              <%})%>
            </ul>
          <%} else{%>
            <p>No messages on the server</p>
          <%}%>
        </div>
        <script src='vendor/recorder.js'></script>
        <script src='javascript/microphone.js'></script>
        <script>
          let microphone = new Microphone();
          microphone.startRecording();
        </script>
